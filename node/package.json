{
  "name": "4gift_node",
  "version": "1.0.0",
  "description": "",
  "main": "server",
  "scripts": {
    "start": "node ./src/server",
    "dev": "NODE_ENV=development nodemon ./src/server.ts",
    "dev:load-envs": "dotenv -e .env.development",
    "dev:migration:generate": "npm run dev:load-envs prisma generate",
    "dev:migration:run": "npm run dev:load-envs prisma migrate dev",
    "dev:seed": "npm run dev:load-envs prisma db seed",
    "test": "npm run dev:load-envs npx jest",
    "coverage": "npm run test -- --coverage"
  },
  "keywords": [
    "gift",
    "birthday",
    "calendar"
  ],
  "author": "ana-pairo",
  "license": "ISC",
  "prisma": {
    "seed": "npm run dev:load-envs -- ts-node prisma/seed.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "http-status": "^1.6.2",
    "pg": "^8.9.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@prisma/client": "^4.9.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@types/pg": "^8.6.6",
    "@types/supertest": "^2.0.12",
    "jest": "^29.4.1",
    "nodemon": "^2.0.20",
    "prisma": "^4.9.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
